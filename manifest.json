{
  "manifest_version": 2,

  "name": "White Noise App",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["whiteNoise.js"],
    "persistent": false
  },

  "content_security_policy": "default-src",
  "permissions": [
  ]
}